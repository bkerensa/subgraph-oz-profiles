{
"name": "chromium"
, "path": "/usr/bin/chromium"
, "reject_user_args": true
, "default_params": [
]
, "xserver": {
	"enabled": true
	, "audio_mode": "pulseaudio"
	, "tray_icon":"/usr/share/icons/hicolor/256x256/apps/chronion.png"
	, "notifications": true
}
, "networking":{
	"type":"bridge"
	, "bridge": "clear"
}
, "whitelist": [
	{"path": "/usr/lib/chromium/chrome-sandbox", "allow_suid": true, "force": true}
	, {"path": "${HOME}/.config/chromium", "can_create": true}
]
, "shared_folders": [
	"${XDG_DOWNLOAD_DIR}"
]
, "blacklist": [
]
, "environment": [
]
, "seccomp": {
	"mode":"disabled"
}
}
